    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_70ssp"
        android:layout_marginLeft="@dimen/_20ssp"
        android:layout_marginTop="@dimen/_100ssp"
        android:layout_marginRight="@dimen/_20ssp"
        android:background="@drawable/item_back"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/_5ssp"
            android:layout_marginTop="@dimen/_5ssp"
            android:layout_marginRight="@dimen/_5ssp"
            android:gravity="start"
            android:orientation="horizontal">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_margin="@dimen/_5ssp"
                android:src="@drawable/ic_baseline_access_time_24"
                app:tint="@color/dark_gray">

            </ImageView>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:text="Time"
                android:textColor="@color/dark_gray"

                android:textSize="@dimen/_18ssp">

            </TextView>
        </LinearLayout>

        <TextView
            android:id="@+id/Time"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginLeft="@dimen/_15ssp"
            android:layout_marginBottom="@dimen/_5ssp"
            android:fontFamily="@font/noor_bold"
            android:text="00.00"
            android:textColor="@color/blue"
            android:textSize="@dimen/_18ssp">

        </TextView>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/dis"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_70ssp"
        android:layout_marginLeft="@dimen/_20ssp"
        android:layout_marginRight="@dimen/_20ssp"
        android:layout_marginTop="@dimen/_20ssp"
        android:background="@drawable/item_back"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/_5ssp"
            android:layout_marginTop="@dimen/_5ssp"
            android:layout_marginRight="@dimen/_5ssp"
            android:gravity="start"
            android:orientation="horizontal">

            <ImageView
                android:layout_width="@dimen/_20ssp"
                android:layout_height="@dimen/_20ssp"
                android:layout_gravity="center"
                android:layout_margin="@dimen/_5ssp"
                android:src="@drawable/distance"
                android:tint="@color/dark_gray"
                app:tint="@color/dark_gray"
                tools:ignore="UseAppTint">

            </ImageView>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:text="Distance"
                android:textColor="@color/dark_gray"
                android:textSize="@dimen/_18ssp">

            </TextView>
        </LinearLayout>

        <TextView
            android:id="@+id/Distance"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginLeft="@dimen/_15ssp"
            android:layout_marginBottom="@dimen/_5ssp"
            android:fontFamily="@font/noor_bold"
            android:text="00    KM"
            android:textColor="@color/blue"
            android:textSize="@dimen/_18ssp">

        </TextView>

    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/_70ssp"
        android:layout_marginLeft="@dimen/_20ssp"
        android:layout_marginRight="@dimen/_20ssp"
        android:layout_marginTop="@dimen/_20ssp"
        android:background="@drawable/item_back"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/dis">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/_5ssp"
            android:layout_marginTop="@dimen/_5ssp"
            android:layout_marginRight="@dimen/_5ssp"
            android:gravity="start"
            android:orientation="horizontal">

            <ImageView
                android:layout_width="@dimen/_20ssp"
                android:layout_height="@dimen/_20ssp"
                android:layout_gravity="center"
                android:layout_margin="@dimen/_5ssp"
                android:src="@drawable/steps"
                android:tint="@color/dark_gray"
                app:tint="@color/dark_gray"
                tools:ignore="UseAppTint">

            </ImageView>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:text="Steps"
                android:textColor="@color/dark_gray"
                android:textSize="@dimen/_18ssp">

            </TextView>
        </LinearLayout>

        <TextView
             android:id="@+id/steps"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginLeft="@dimen/_15ssp"
            android:layout_marginBottom="@dimen/_5ssp"
            android:fontFamily="@font/noor_bold"
            android:text="00    KM"
            android:textColor="@color/blue"
            android:textSize="@dimen/_18ssp">

        </TextView>

    </LinearLayout>
    <TextView
         android:id="@+id/Start"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_35ssp"
        android:text="Start"
        android:textSize="@dimen/_15ssp"
        android:background="@drawable/button"
        android:layout_marginRight="@dimen/_20ssp"
        android:layout_marginLeft="@dimen/_20ssp"
        android:gravity="center"
        android:textColor="@color/white"
        android:layout_marginBottom="@dimen/_30ssp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

    </TextView>
    <include
        layout="@layout/start_action"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

    </include>

